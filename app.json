{
  "expo": {
    "name": "Daivajnya-brahmin",
    "slug": "Daivajnya-brahmin",
    "version": "2.1.5",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.svgrtech.daivjnayabrahmin"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android": {
      "package": "com.svgrtech.daivajnyabrahmin",
      "useNextNotificationsApi": true,
      "googleServicesFile": "./google-services.json"
    },
    "notification": {
      "icon": "./assets/icon-2.png"
    }
  }
}
